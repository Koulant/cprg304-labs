package exercise_2;

public class Driver {

	public static void main(String[] args) {
		
		//Exercise 2
		Integer[] sortedArray = {2, 13, 24, 35, 46, 57, 69, 78, 81, 95};
		int target = 1;
		
		int result = BinarySearch.binarySearch(sortedArray, target);
				
		if (result != -1) {
			System.out.println("Element " + target + " found at index " + result + ".");

		}
		else {
			System.out.println("-1. \nElement " + target + " not found in the array.");
		}

	}
}
